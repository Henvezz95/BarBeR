{
    "coco_annotations_path": "./annotations/COCO/",
    "class": "2D",
    "single_ROI": true,
    "longest_edge_resize":640,
    "algorithms":[
        {
            "name": "Soros",
            "library": "soros_detector",
            "class": "Soros_detector",
            "args": {
                "lib_path": "./build/Gallo2011-Soros2013-Yun2017/libBarcodeLocalization.so",
                "winsize": 20,
                "detectionType":"2D"
            }
        },
        {
            "name": "Yolo nano",
            "library": "ultralytics_detector",
            "class": "YOLO_detector",
            "args": {
                "model_path": "./Saved Models/yolov8n_640_2cl.onnx",
                "imgsz": 640
            }
        },
        {
            "name": "Yolo small",
            "library": "ultralytics_detector",
            "class": "YOLO_detector",
            "args": {
                "model_path": "./Saved Models/yolov8s_640_2cl.onnx",
                "imgsz": 640
            }
        },
        {
            "name": "Faster RCNN",
            "library": "detectron2_detector",
            "class": "Detectron2_detector",
            "args": {
                "model_path": "./Saved Models/Detectron2_models/model_final.pth"
            }
        }
    ]
}